        // Blocca TUTTI i popup del browser immediatamente
        (function() {
            // Salva le funzioni originali
            var originalAlert = window.alert;
            var originalConfirm = window.confirm;
            var originalPrompt = window.prompt;
            
            // Sovrascrivi tutte le funzioni popup
            window.alert = function(message) {
                console.log('ALERT BLOCCATO:', message);
                return;
            };
            
            window.confirm = function(message) {
                console.log('CONFIRM BLOCCATO:', message);
                return true; // Ritorna sempre true
            };
            
            window.prompt = function(message, defaultText) {
                console.log('PROMPT BLOCCATO:', message);
                return defaultText || '';
            };
            
            console.log('ðŸš« Tutti i popup del browser sono stati bloccati');
            
            // Opzione per riabilitare i popup se necessario
            window.enablePopups = function() {
                window.alert = originalAlert;
                window.confirm = originalConfirm;
                window.prompt = originalPrompt;
                console.log('âœ… Popup del browser riabilitati');
            };
        })();
        </script>
