    const fieldId = 'caricare_gli_estratti_conto';
    let initialized = false;

    function initializeDragDrop() {
        if (initialized) {
            console.log(`Drag-and-drop for ${fieldId} already initialized, skipping...`);
            return;
        }

        if ($(`#${fieldId}`).length) {
            console.log(`File input ${fieldId} found, adding drag-and-drop area`);
            initialized = true;

            $(`#${fieldId}`).addClass(`file-field-${fieldId}`);
            $(`#${fieldId}`).wrap(`<div class="custom-drag-drop custom-drag-drop-${fieldId}"></div>`);
            $(`#${fieldId}`).parent().append(`
                <div class="drag-drop-areas drag-drop-areas-${fieldId}">
                    <span class="drag-drop-icon">
                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24"
     viewBox="0 0 24 24" style="fill: #b91c1c;">
  <path d="M11 15h2V9h3l-4-5-4 5h3z"></path>
  <path d="M20 18H4v-7H2v7c0 1.103.897 2 2 2h16c1.103 0 2-.897 2-2v-7h-2v7z"></path>
</svg>

                    </span>
                    <span class="drag-drop-text">Caricare gli estratti conto</span>
                    <span class="file-extension">.pdf</span>
                </div>
                <div class="file-list file-list-${fieldId}"></div>
            `);

            const dropArea = $(`.drag-drop-areas-${fieldId}`);
            const fileInput = $(`#${fieldId}`);
            const fileList = $(`.file-list-${fieldId}`);

            dropArea.off('click').on('click', function(e) {
                e.preventDefault();
                e.stopPropagation();
                console.log(`Drag-drop areas for ${fieldId} clicked, triggering file input`);
                fileInput.click();
            });

            dropArea.off('dragover').on('dragover', function(e) {
                e.preventDefault();
                $(this).addClass('dragover');
            });

            dropArea.off('dragleave').on('dragleave', function(e) {
                e.preventDefault();
                $(this).removeClass('dragover');
            });

            dropArea.off('drop').on('drop', function(e) {
                e.preventDefault();
                $(this).removeClass('dragover');
                const files = e.originalEvent.dataTransfer.files;
                fileInput[0].files = files;
                displayFiles(files);
            });

            fileInput.off('change').on('change', function() {
                console.log(`File input ${fieldId} changed, displaying files`);
                displayFiles(this.files);
            });

            function displayFiles(files) {
                fileList.empty();
                for (let i = 0; i < files.length; i++) {
                    const file = files[i];
                    const fileSize = (file.size / 1024).toFixed(2) + ' kb';
                    const fileItem = `
                        <div class="file-item">
                            <img src="https://cdn-icons-png.flaticon.com/512/136/136522.png" alt="File Icon">
                            <span>${file.name}</span>
                            <span class="file-size">${fileSize}</span>
                            <span class="check-icon">âœ”</span>
                        </div>`;
                    fileList.append(fileItem);
                }
            }
        } else {
            console.log(`File input #${fieldId} not found, retrying...`);
            setTimeout(initializeDragDrop, 500);
        }
    }

    initializeDragDrop();
});
