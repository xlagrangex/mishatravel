<script id="jquery-js-after">
jQuery(document).ready(function($) {
    var $form = $('.misha-booking-form');
    var $dataPartenza = $('#data_partenza');
    var $hotelStar = $('#hotel_star');
    var $numeroPersone = $('#numero_persone');
    var $prezzoPersona = $('#misha-prezzo-persona');
    var $prezzoTotale = $('#misha-prezzo-totale');
    var postId = $form.data('post-id');
    var postType = $form.data('post-type');

    function updatePriceAndHotels() {
        var dataPartenza = $dataPartenza.val();
        var hotelStar = $hotelStar.val();
        var numeroPersone = parseInt($numeroPersone.val()) || 1;

        if (!dataPartenza || !hotelStar) {
            $prezzoPersona.text('');
            $prezzoTotale.text('');
            $('.hotel-select').each(function() {
                $(this).html('<option value="">' + (postType === 'crociere' ? mishaBooking.i18n.select_cabin : mishaBooking.i18n.select_hotel) + '</option>');
            });
            return;
        }

        // Fetch price
        $.ajax({
            url: mishaBooking.ajax_url,
            type: 'POST',
            data: {
                action: 'misha_get_price',
                nonce: mishaBooking.nonce,
                post_id: postId,
                post_type: postType,
                data_partenza: dataPartenza,
                hotel_star: hotelStar,
                numero_persone: numeroPersone
            },
            success: function(response) {
                if (response.success) {
                    if (response.data.price_per_person === 'Prezzo su richiesta') {
                        $prezzoPersona.html('<span class="price-on-request" style="color:#0070f3;font-weight:bold;">' + response.data.price_per_person + '</span>');
                        $prezzoTotale.html('<span class="price-on-request" style="color:#0070f3;font-weight:bold;">' + response.data.total_price + '</span>');
                    } else {
                        $prezzoPersona.text('â‚¬' + response.data.price_per_person);
                        $prezzoTotale.text('â‚¬' + response.data.total_price);
                    }
                } else {
                    $prezzoPersona.text('');
                    $prezzoTotale.text('');
                    alert(response.data.message || mishaBooking.i18n.price_error);
                }
            },
            error: function() {
                $prezzoPersona.text('');
                $prezzoTotale.text('');
                alert(mishaBooking.i18n.price_error);
            }
        });

        // Fetch hotels
        $.ajax({
            url: mishaBooking.ajax_url,
            type: 'POST',
            data: {
                action: 'misha_get_hotels',
                nonce: mishaBooking.nonce,
                post_id: postId,
                post_type: postType,
                hotel_star: hotelStar
            },
            success: function(response) {
                if (response.success) {
                    $('.hotel-select').each(function() {
                        var $select = $(this);
                        var location = $select.closest('.hotel-group').data('location');
                        $select.html('<option value="">' + (postType === 'crociere' ? mishaBooking.i18n.select_cabin : mishaBooking.i18n.select_hotel) + '</option>');
                        
                        if (response.data.hotels[location]) {
                            $.each(response.data.hotels[location], function(index, hotel) {
                                $select.append('<option value="' + hotel.nome_albergo + '">' + hotel.nome_albergo + '</option>');
                            });
                        }
                    });
                } else {
                    $('.hotel-select').each(function() {
                        $(this).html('<option value="">' + (postType === 'crociere' ? mishaBooking.i18n.select_cabin : mishaBooking.i18n.select_hotel) + '</option>');
                    });
                    alert(response.data.message || mishaBooking.i18n.hotels_error);
                }
            },
            error: function() {
                $('.hotel-select').each(function() {
                    $(this).html('<option value="">' + (postType === 'crociere' ? mishaBooking.i18n.select_cabin : mishaBooking.i18n.select_hotel) + '</option>');
                });
                alert(mishaBooking.i18n.hotels_error);
            }
        });
    }

    $dataPartenza.on('change', updatePriceAndHotels);
    $hotelStar.on('change', updatePriceAndHotels);
    $numeroPersone.on('input', updatePriceAndHotels);

    // Initialize on page load
    if ($dataPartenza.val() && $hotelStar.val()) {
        updatePriceAndHotels();
    }
});
</script>
