                        <div class="progress-fill"></div>
                    </div>
                    <div class="percentage" id="percentage">0%</div>
                </div>

                <!-- Dots Animation Subtle -->
                <div class="dots-container">
                    <span class="dot"></span>
                    <span class="dot"></span>
                    <span class="dot"></span>
                    <span class="dot"></span>
                    <span class="dot"></span>
                </div>
            </div>
